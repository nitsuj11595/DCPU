;;; List of all hardware id's
:MONITOR_LEM
	DAT	0xF615, 0x734D	;id for LEM monitor
:MONITOR_PIXIE
	DAT	0xF615, 0x774D	;id for PIXIE monitor

:CLOCK_GENERIC
	DAT  0xb402, 0x12d0 	;id for Generic Clock
